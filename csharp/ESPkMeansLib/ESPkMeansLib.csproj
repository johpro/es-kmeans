<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <Version>1.2.0</Version>
	  <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <Authors>Johannes Knittel</Authors>
	  <Company />
    <Copyright>Copyright(c) Johannes Knittel</Copyright>
    <PackageProjectUrl>https://github.com/johpro/esp-kmeans</PackageProjectUrl>
    <RepositoryUrl>https://github.com/johpro/esp-kmeans</RepositoryUrl>
	  <Title>ESP-kMeans</Title>
    <Description>ESP-kMeans is a fast and easy-to-use clustering library for text and numerical data, with efficient implementations of k-Means++ and Spherical k-Means++. It also provides an indexing structure for efficiently retrieving nearest neighbors based on the dot product.</Description>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

</Project>
